    $(position).css('position','relative');
    if ( position == 'html' ) { $(position).css("overflow","hidden"); }
    $(position).append(html);
  }
  function loaderclose(position) {
    if ( position == '' || position == null ) {
      $('.loader').remove();
    } else {
      if ( $(position).children('.loader').length > 0 ) {
        if ( position == "html" ) { $(position).css('overflow','unset'); $(position).css("position",""); }
        $(position).children('.loader').remove();
      }
    }
  }
  // loader('html');
</script>
